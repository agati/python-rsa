def verifica_primo(n):

    print "Inicio:",inicio
    d=3
    x=int(sqrt(n))
    if n % 2 == 0:
        print "E par"
        return False

    while d <= x:

        print "d vale:",d
        if n % d == 0:
            print d, '*', n / d, '=', n
            return (d,n/d,n)
        else:
            d += 2


    return False



print"Algoritmo de forca bruta..."
